{
    "name": "ep-bot",
    "license": "MIT",
    "type": "module",
    "engines": {
        "node" : ">=16.0.0"
    },
    "scripts": {
        "build": "tsc --project tsconfig.json",
        "watch": "tsc --project tsconfig.json --watch",
        "start": "node bin/main.js",
        "dev": "ts-node src/main.ts --test",
        "dev-now": "yarn build && yarn start",
        "lint": "eslint \"src/**/*.ts\"",
        "prettify": "eslint --fix --ext .ts src"
    },
    "devDependencies": {
        "@types/aes-js": "3.1.1",
        "@types/node": "17.0.21",
        "@typescript-eslint/eslint-plugin": "5.15.0",
        "@typescript-eslint/parser": "5.15.0",
        "dotenv": "16.0.0",
        "eslint": "8.11.0",
        "eslint-import-resolver-typescript": "2.5.0",
        "eslint-plugin-import": "2.25.4",
        "ts-node": "10.7.0",
        "ts-node-dev": "1.1.8",
        "typescript": "4.6.3"
    },
    "dependencies": {
        "@babakness/exhaustive-type-checking": "0.1.3",
        "app-module-path": "2.2.0",
        "arg": "5.0.1",
        "fp-ts": "2.12.0",
        "lowdb": "3.0.0",
        "telegraf": "4.7.0",
        "typegram": "3.8.0"
    }
}
